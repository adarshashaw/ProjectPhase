{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryfalcon"
		},
		"ExcelFileServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ExcelFileServerLS'"
		},
		"SqlDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDatabaseLS'"
		},
		"ExcelFileServerLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\CASE STUDY 3"
		},
		"ExcelFileServerLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Adarsha"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryfalcon_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ExcelFileServerLS_password": {
						"value": "[parameters('ExcelFileServerLS_password')]"
					},
					"SqlDatabaseLS_connectionString": {
						"value": "[parameters('SqlDatabaseLS_connectionString')]"
					},
					"ExcelFileServerLS_properties_typeProperties_host": {
						"value": "[parameters('ExcelFileServerLS_properties_typeProperties_host')]"
					},
					"ExcelFileServerLS_properties_typeProperties_userId": {
						"value": "[parameters('ExcelFileServerLS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}